    "def simulate_3d_segmentation(xray, depth_map, depth_slices=32):\n",
    "    \"\"\"Simulate 3D dental segmentation (ResUNet3D simulation).\"\"\"\n",
    "    height, width = xray.shape\n",
    "    \n",
    "    # Create 3D volume from depth information\n",
    "    min_depth, max_depth = depth_map.min(), depth_map.max()\n",
    "    depth_range = max_depth - min_depth\n",
    "    \n",
    "    # Initialize 3D segmentation volume\n",
    "    segmentation_3d = np.zeros((depth_slices, height, width), dtype=np.uint8)\n",
    "    \n",
    "    # Define tooth classes (simplified)\n",
    "    tooth_classes = {\n",
    "        0: 'background',\n",
    "        1: 'incisor',\n",
    "        2: 'canine', \n",
    "        3: 'premolar',\n",
    "        4: 'molar',\n",
    "        5: 'root',\n",
    "        6: 'crown'\n",
    "    }\n",
    "    \n",
    "    # For each pixel, determine which depth slices it occupies\n",
    "    for y in range(height):\n",
    "        for x in range(width):\n",
    "            pixel_depth = depth_map[y, x]\n",
    "            pixel_intensity = xray[y, x]\n",
    "            \n",
    "            # Calculate which slice this pixel maps to\n",
    "            slice_idx = int((pixel_depth - min_depth) / depth_range * (depth_slices - 1))\n",
    "            slice_idx = max(0, min(depth_slices - 1, slice_idx))\n",
    "            \n",
    "            # Assign tooth class based on intensity and position\n",
    "            if pixel_intensity > 200:  # High intensity = tooth structure\n",
    "                # Determine tooth type based on position\n",
    "                if x < width * 0.3 or x > width * 0.7:  # Sides\n",
    "                    if pixel_intensity > 230:\n",
    "                        tooth_class = 2  # canine\n",
    "                    else:\n",
    "                        tooth_class = 4  # molar\n",
    "                elif width * 0.4 <= x <= width * 0.6:  # Center\n",
    "                    tooth_class = 1  # incisor\n",
    "                else:  # Between center and sides\n",
    "                    tooth_class = 3  # premolar\n",
    "                \n",
    "                # Assign to current slice and adjacent slices for thickness\n",
    "                for offset in [-1, 0, 1]:\n",
    "                    target_slice = slice_idx + offset\n",
    "                    if 0 <= target_slice < depth_slices:\n",
    "                        segmentation_3d[target_slice, y, x] = tooth_class\n",
    "                        \n",
    "            elif pixel_intensity > 150:  # Medium intensity = root structures\n",
    "                segmentation_3d[slice_idx, y, x] = 5  # root\n",
    "    \n",
    "    return segmentation_3d, tooth_classes\n",
    "\n",
    "# Simulate 3D segmentation\n",
    "segmentation_3d, tooth_classes = simulate_3d_segmentation(synthetic_xray, simulated_depth)\n",
    "print(f\"‚úÖ Generated 3D segmentation with shape: {segmentation_3d.shape}\")\n",
    "print(f\"   Tooth classes: {tooth_classes}\")\n",
    "\n",
    "# Display different slices of the 3D segmentation\n",
    "slice_indices = [8, 16, 24]  # Sample slices\n",
    "fig, axes = plt.subplots(1, len(slice_indices), figsize=(15, 5))\n",
    "\n",
    "for i, slice_idx in enumerate(slice_indices):\n",
    "    im = axes[i].imshow(segmentation_3d[slice_idx], cmap='tab10')\n",
    "    axes[i].set_title(f'3D Segmentation - Slice {slice_idx}', fontsize=12, fontweight='bold')\n",
    "    axes[i].axis('off')\n",
    "\n",
    "# Add colorbar\n",
    "plt.colorbar(im, ax=axes[-1], fraction=0.046, pad=0.04, label='Tooth Class')\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# Show class distribution\n",
    "unique_classes, counts = np.unique(segmentation_3d, return_counts=True)\n",
    "class_names = [tooth_classes.get(cls, f'Class {cls}') for cls in unique_classes]\n",
    "\n",
    "plt.figure(figsize=(10, 6))\n",
    "bars = plt.bar(class_names, counts, color=plt.cm.tab10(unique_classes))\n",
    "plt.title('Distribution of Segmented Tooth Classes', fontsize=14, fontweight='bold')\n",
    "plt.xlabel('Tooth Class')\n",
    "plt.ylabel('Number of Voxels')\n",
    "plt.xticks(rotation=45)\n",
    "\n",
    "# Add value labels on bars\n",
    "for bar, count in zip(bars, counts):\n",
    "    plt.text(bar.get_x() + bar.get_width()/2, bar.get_height() + max(counts)*0.01,\n",
    "             f'{count:,}', ha='center', va='bottom', fontweight='bold')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
