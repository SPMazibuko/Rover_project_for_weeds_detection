    "def create_synthetic_xray(width=512, height=384):\n",
    "    \"\"\"Create a synthetic panoramic X-ray for demonstration.\"\"\"\n",
    "    # Create base noise\n",
    "    np.random.seed(42)  # For reproducibility\n",
    "    xray = np.random.normal(0.3, 0.1, (height, width))\n",
    "    \n",
    "    # Add dental arch structure\n",
    "    y, x = np.ogrid[:height, :width]\n",
    "    \n",
    "    # Upper dental arch (parabolic shape)\n",
    "    arch_center_y = height * 0.3\n",
    "    arch_width = width * 0.8\n",
    "    arch_height = height * 0.15\n",
    "    \n",
    "    for i in range(width):\n",
    "        if width * 0.1 <= i <= width * 0.9:\n",
    "            # Parabolic curve for dental arch\n",
    "            normalized_x = (i - width/2) / (width/2)\n",
    "            arch_y = arch_center_y + arch_height * (normalized_x ** 2)\n",
    "            \n",
    "            # Add dental arch intensity\n",
    "            y_start = max(0, int(arch_y - 10))\n",
    "            y_end = min(height, int(arch_y + 10))\n",
    "            xray[y_start:y_end, i] += 0.4\n",
    "    \n",
    "    # Lower dental arch\n",
    "    arch_center_y = height * 0.7\n",
    "    for i in range(width):\n",
    "        if width * 0.1 <= i <= width * 0.9:\n",
    "            normalized_x = (i - width/2) / (width/2)\n",
    "            arch_y = arch_center_y - arch_height * 0.7 * (normalized_x ** 2)\n",
    "            \n",
    "            y_start = max(0, int(arch_y - 8))\n",
    "            y_end = min(height, int(arch_y + 8))\n",
    "            xray[y_start:y_end, i] += 0.3\n",
    "    \n",
    "    # Add individual teeth (simplified rectangles)\n",
    "    teeth_positions = [\n",
    "        # Upper teeth\n",
    "        (width*0.2, height*0.25, 15, 25),\n",
    "        (width*0.3, height*0.22, 18, 30),\n",
    "        (width*0.4, height*0.20, 20, 35),\n",
    "        (width*0.5, height*0.19, 20, 35),\n",
    "        (width*0.6, height*0.20, 20, 35),\n",
    "        (width*0.7, height*0.22, 18, 30),\n",
    "        (width*0.8, height*0.25, 15, 25),\n",
    "        # Lower teeth\n",
    "        (width*0.2, height*0.65, 15, 25),\n",
    "        (width*0.3, height*0.67, 18, 30),\n",
    "        (width*0.4, height*0.68, 20, 35),\n",
    "        (width*0.5, height*0.69, 20, 35),\n",
    "        (width*0.6, height*0.68, 20, 35),\n",
    "        (width*0.7, height*0.67, 18, 30),\n",
    "        (width*0.8, height*0.65, 15, 25),\n",
    "    ]\n",
    "    \n",
    "    for x_pos, y_pos, w, h in teeth_positions:\n",
    "        x_start = max(0, int(x_pos - w//2))\n",
    "        x_end = min(width, int(x_pos + w//2))\n",
    "        y_start = max(0, int(y_pos - h//2))\n",
    "        y_end = min(height, int(y_pos + h//2))\n",
    "        \n",
    "        xray[y_start:y_end, x_start:x_end] += 0.5\n",
    "    \n",
    "    # Clip values and convert to 0-255 range\n",
    "    xray = np.clip(xray, 0, 1)\n",
    "    xray = (xray * 255).astype(np.uint8)\n",
    "    \n",
    "    return xray\n",
    "\n",
    "# Create synthetic X-ray\n",
    "synthetic_xray = create_synthetic_xray()\n",
    "print(f\"‚úÖ Created synthetic X-ray with shape: {synthetic_xray.shape}\")\n",
    "\n",
    "# Display the synthetic X-ray\n",
    "plt.figure(figsize=(12, 8))\n",
    "plt.imshow(synthetic_xray, cmap='gray')\n",
    "plt.title('Synthetic Panoramic Dental X-ray', fontsize=16, fontweight='bold')\n",
    "plt.axis('off')\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# Show intensity histogram\n",
    "plt.figure(figsize=(10, 4))\n",
    "plt.hist(synthetic_xray.flatten(), bins=50, alpha=0.7, color='blue')\n",
    "plt.title('Pixel Intensity Distribution')\n",
    "plt.xlabel('Pixel Intensity')\n",
    "plt.ylabel('Frequency')\n",
    "plt.grid(True, alpha=0.3)\n",
    "plt.show()"
   ]
