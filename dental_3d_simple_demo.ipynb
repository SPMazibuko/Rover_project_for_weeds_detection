    "def simulate_depth_estimation(xray):\n",
    "    \"\"\"Simulate depth estimation from X-ray (DepthGAN simulation).\"\"\"\n",
    "    # Simple depth simulation based on intensity gradients\n",
    "    height, width = xray.shape\n",
    "    \n",
    "    # Create depth map based on X-ray intensity and position\n",
    "    depth_map = np.zeros_like(xray, dtype=np.float32)\n",
    "    \n",
    "    # Base depth based on intensity (brighter = closer)\n",
    "    normalized_xray = xray.astype(np.float32) / 255.0\n",
    "    depth_map = 10.0 + normalized_xray * 15.0  # Depth in mm (10-25mm range)\n",
    "    \n",
    "    # Add anatomical depth variation\n",
    "    y, x = np.ogrid[:height, :width]\n",
    "    \n",
    "    # Teeth are generally at similar depth\n",
    "    # Add slight variation based on position in dental arch\n",
    "    center_x, center_y = width // 2, height // 2\n",
    "    \n",
    "    # Distance from center affects depth slightly\n",
    "    distance_from_center = np.sqrt((x - center_x)**2 + (y - center_y)**2)\n",
    "    max_distance = np.sqrt(center_x**2 + center_y**2)\n",
    "    depth_variation = (distance_from_center / max_distance) * 3.0\n",
    "    \n",
    "    depth_map += depth_variation\n",
    "    \n",
    "    # Add some noise for realism\n",
    "    np.random.seed(42)\n",
    "    noise = np.random.normal(0, 0.5, depth_map.shape)\n",
    "    depth_map += noise\n",
    "    \n",
    "    return depth_map\n",
    "\n",
    "# Simulate depth estimation\n",
    "simulated_depth = simulate_depth_estimation(synthetic_xray)\n",
    "print(f\"‚úÖ Generated depth map with range: {simulated_depth.min():.2f} - {simulated_depth.max():.2f} mm\")\n",
    "\n",
    "# Display X-ray and depth map side by side\n",
    "fig, axes = plt.subplots(1, 2, figsize=(16, 6))\n",
    "\n",
    "# Original X-ray\n",
    "axes[0].imshow(synthetic_xray, cmap='gray')\n",
    "axes[0].set_title('Input: Panoramic X-ray', fontsize=14, fontweight='bold')\n",
    "axes[0].axis('off')\n",
    "\n",
    "# Depth map\n",
    "im = axes[1].imshow(simulated_depth, cmap='viridis')\n",
    "axes[1].set_title('Output: Estimated Depth Map', fontsize=14, fontweight='bold')\n",
    "axes[1].axis('off')\n",
    "plt.colorbar(im, ax=axes[1], fraction=0.046, pad=0.04, label='Depth (mm)')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
